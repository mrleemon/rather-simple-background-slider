{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "occ/rather-simple-background-slider",
	"title": "Rather Simple Background Slider",
    "description": "Display a background slider.",
	"textdomain": "rather-simple-background-slider",
	"category": "media",
    "attributes": {
		"images": {
			"type": "array",
			"items": {
				"type": "object"
			},
			"default": []
		},
		"delay": {
			"type": "integer",
			"default": 5
		}
    },
	"supports": {
		"html": false,
		"multiple": false
	},
    "editorScript": "file:./index.js",
	"viewScript": "file:./view.js",
	"style": [
		"file:./style-index.css",
		"file:./view.css"
	],
	"editorStyle": "file:./index.css",
	"render": "file:./render.php"
}